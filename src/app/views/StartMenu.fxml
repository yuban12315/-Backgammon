<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.*?>
<?scenebuilder-background-color 0xdadadaff?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1"
            xmlns="http://javafx.com/javafx/2.2" fx:controller="app.controllers.StartCtrl">
    <children>
        <GridPane alignment="CENTER" layoutX="189.0" layoutY="45.0" minHeight="100.0" prefHeight="311.0"
                  prefWidth="222.0">
            <children>
                <Button id="OfflineBtn" alignment="TOP_LEFT" blendMode="EXCLUSION" cancelButton="false"
                        contentDisplay="CENTER" defaultButton="false" minWidth="150.0" mnemonicParsing="false"
                        onAction="#offlineGame" text="单机对战" textAlignment="CENTER" textFill="BLACK"
                        textOverrun="LEADING_WORD_ELLIPSIS" wrapText="false" GridPane.columnIndex="0"
                        GridPane.halignment="CENTER" GridPane.rowIndex="0" GridPane.valignment="CENTER">
                    <font>
                        <Font name="Century Gothic Bold" size="30.0" fx:id="x1"/>
                    </font>
                </Button>
                <Button id="OnlineBtn" alignment="TOP_LEFT" blendMode="EXCLUSION" cancelButton="false"
                        contentDisplay="CENTER" defaultButton="false" font="$x1" minWidth="170.0"
                        mnemonicParsing="false" onAction="#onlineGame" text="联机对战" textAlignment="CENTER"
                        textFill="BLACK" textOverrun="LEADING_WORD_ELLIPSIS" wrapText="false" GridPane.columnIndex="0"
                        GridPane.halignment="CENTER" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </Button>
                <Button id="ExitBtn" alignment="CENTER" contentDisplay="CENTER" minWidth="170.0" mnemonicParsing="false"
                        text="退出" textAlignment="CENTER" textFill="BLACK" GridPane.columnIndex="0"
                        GridPane.halignment="CENTER" GridPane.rowIndex="2" onAction="#exit">
                    <font>
                        <Font name="Century Gothic" size="30.0"/>
                    </font>
                    <GridPane.margin>
                        <Insets top="20.0"/>
                    </GridPane.margin>
                </Button>
            </children>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <effect>
                <ColorAdjust/>
            </effect>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
        </GridPane>
    </children>
</AnchorPane>